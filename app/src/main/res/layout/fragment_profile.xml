        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/spacing_larger"
            android:layout_marginStart="@dimen/spacing_larger"
            android:layout_marginBottom="@dimen/spacing_normal"
            android:layout_marginTop="@dimen/spacing_normal"
            android:paddingRight="@dimen/spacing_larger"
            android:paddingEnd="@dimen/spacing_larger"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txt_vk_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:fontFamily="@string/ff_regular"
                android:gravity="center_vertical|center_horizontal"
                android:text="@string/fragment.profile.vk.value"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="@dimen/font_large"
                tools:ignore="RtlCompat,UnusedAttribute" />

            <TextView
                android:id="@+id/txt_vk_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
